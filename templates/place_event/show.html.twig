{% extends 'base.html.twig' %}

{% block title %}Détails du lieu de l'évènement{% endblock %}

{% block content %}
    <h1>Lieu de l'évènement</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ placeEvent.id }}</td>
            </tr>
            <tr>
                <th>Quartier</th>
                <td>{{ placeEvent.districtPlaceEvent }}</td>
            </tr>
            <tr>
                <th>Label </th>
                <td>{{ placeEvent.labelPlaceEvent }}</td>
            </tr>
            <tr>
                <th>ville r</th>
                <td>{{ placeEvent.cityPlaceEvent }}</td>
            </tr>
            <tr>
                <th>code postal</th>
                <td>{{ placeEvent.zipCodePlaceEvent }}</td>
            </tr>
            <tr>
                <th>pays r</th>
                <td>{{ placeEvent.countryPlaceEvent }}</td>
            </tr>
        </tbody>
    </table>

<a href="{{ path('placeEvent_index') }}">Revenir à la liste</a>

<a href="{{ path('placeEvent_edit', {'id': post.id}) }}">Modifier</a>

{{ include('placeEvent/_delete_form.html.twig') }}
{% endblock %}
